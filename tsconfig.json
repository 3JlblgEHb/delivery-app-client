{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
	  "jsx": "react-native",
	  "isolatedModules": false,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "allowJs": true,
	  "skipLibCheck": true,
	  "noEmit": true,
	  "allowSyntheticDefaultImports": true,
	  "strict": true,
	  "baseUrl": ".",
	  "paths": {
		"@/*": ["./app/*"]
	  }
	}
  }
  